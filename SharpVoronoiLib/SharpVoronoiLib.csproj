<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <Version>1.0.4</Version>
        <AssemblyVersion>1.0.0</AssemblyVersion>
        <ImplicitUsings>disable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <LangVersion>13</LangVersion>
        <TargetFrameworks>netstandard2.0;net9.0</TargetFrameworks>
        <Title>SharpVoronoiLib</Title>
        <Authors>various</Authors>
        <Description>Generate a Voronoi diagram from a set of points in a plane (using Fortune's Algorithm) with edge clipping and border closure. Includes Lloyds relaxation algorithm. Original version published as VoronoiLib.</Description>
        <Copyright>MIT License, various authors</Copyright>
        <PackageProjectUrl>https://github.com/RudyTheDev/SharpVoronoiLib</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/RudyTheDev/SharpVoronoiLib/blob/main/License.md</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/RudyTheDev/SharpVoronoiLib</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>voronoi lloyds fortune's tessellation </PackageTags>
        <PackageReleaseNotes></PackageReleaseNotes>
        <PackageReadmeFile>README.md</PackageReadmeFile>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="JetBrains.Annotations" Version="2024.3.0">
            <PrivateAssets>all</PrivateAssets>
        </PackageReference>
        <None Include="..\README.md" Pack="true" PackagePath="\"/>
    </ItemGroup>

    <ItemGroup>
      <Folder Include="Nearest Site Lookup\KDTree\" />
    </ItemGroup>

</Project>
